    <modules>
        <module>x-pai-api</module>
        <module>x-pai-dal</module>
        <module>x-pai-biz</module>
        <module>x-pai-service</module>
        <module>x-pai-engine</module>
        <module>x-pai-serving</module>
        <module>pai-common</module>
        <module>pai-admin</module>
    </modules>
    <distributionManagement>
        <repository>
            <id>tigerye-release</id>
            <name>Release Repository of Tigerye</name>
            <url>http://maven.aigauss.com/repository/tigerye-release/</url>
        </repository>
        <snapshotRepository>
            <id>tigerye-snapshot</id>
            <name>Snapshot Repository of Tigerye</name>
            <url>http://maven.aigauss.com/repository/tigerye-snapshot/</url>
        </snapshotRepository>
    </distributionManagement>
